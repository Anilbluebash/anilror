<%= form_for([@college, @college.students.build]) do |f| %>
  <p>
    <%= f.label :Name %><br>
    <%= f.text_field :Name %>
  </p>
 
  <p>
    <%= f.label :age %><br>
    <%= f.number_field :age %>
  </p>

  <p>
    <%= f.label :marks %><br>
    <%= f.text_area :marks %>
  </p>

  <p>
    <%= f.label :rollnumber %><br>
    <%= f.text_area :rollnumber %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>